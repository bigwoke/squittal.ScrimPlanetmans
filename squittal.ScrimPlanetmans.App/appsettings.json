{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
            //"Microsoft.AspNetCore.SignalR": "Debug",
            //"Microsoft.AspNetCore.Http.Connections": "Debug"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "PlanetmansDbContext": "Server=(LocalDB)\\MSSQLLocalDB;Database=PlanetmansDbContext;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "Keybinds": {
        // Integer value from https://learn.microsoft.com/en-us/dotnet/api/system.windows.forms.keys
        "StartMatch": 192
    },
    "KeyModifiers": {
        // None = 0, Alt = 1, Ctrl = 2, Shift = 4, Windows (not recommended) = 8
        // Add values for multiple modifiers. Ex: Ctrl+Alt = 3
        "StartMatch": 1
    }
}
